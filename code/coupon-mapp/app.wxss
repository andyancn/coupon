.dialog {
  position: fixed;
  top: 0;
  left: 0;
  bottom: 0;
  right: 0;
  z-index: 999;
}

.dialog .mask {
  position: absolute;
  top: 0;
  left: 0;
  bottom: 0;
  right: 0;
  z-index: 10;
  background-color: rgba(0, 0, 0, .6);
}

.dialog .dialog-box {
  position: relative;
  z-index: 12;
  height: 100%;
}

.dialog .dialog-box .dialog-body {
  width: 100%;
  box-sizing: border-box;
  padding: 20px 28px;
  position: absolute;
  z-index: 12;
  top: 40%;
  margin-top: -50%;
}

.dialog .dialog-content {
  width: 80%;
  min-height: 200rpx;
  background-color: #fff;
  padding: 14px 10px;
  border-radius: 5px;
}



